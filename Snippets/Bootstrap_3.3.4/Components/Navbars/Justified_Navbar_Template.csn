<?xml version="1.0" encoding="utf-8"?><snippet name="Justified Navbar Template" description="Bootstrap 3.3.4 Justified Navbar Template" preview="code" type="block"><insertText location="beforeSelection"><![CDATA[      <!-- The justified navigation menu is meant for single line per list item.           Multiple lines will require custom code not provided by Bootstrap. -->      <div class="masthead">        <h3 class="text-muted">Project name</h3>        <nav>          <ul class="nav nav-justified">            <li class="active"><a href="#">Home</a></li>            <li><a href="#">Projects</a></li>            <li><a href="#">Services</a></li>            <li><a href="#">Downloads</a></li>            <li><a href="#">About</a></li>            <li><a href="#">Contact</a></li>          </ul>        </nav>      </div>]]></insertText><insertText location="afterSelection"><![CDATA[]]></insertText></snippet>